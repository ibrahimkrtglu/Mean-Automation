<div class="container">
    <div class="row">
    <div class="loginPanel">
        <div class="panel panel-info">
            <div class="panel-heading">
                <div class="panel-title">Yonetici Ekle</div>
            </div>

            <div class="panel-body">
                <form action="/yoneticiekle" method="POST">
                <!-- ad -->
                <div style="margin-bottom: 25px" class="input-group">
                    <span class="input-group-addon"><i class="glyphicon glyphicon-user"></i></span>
                    <input id="adminname" type="text" class="form-control" name="ad" placeholder="Yönetici Adı">
                </div>
                <!-- soyad -->
                <div style="margin-bottom: 25px" class="input-group">
                    <span class="input-group-addon"><i class="glyphicon glyphicon-user"></i></span>
                    <input id="adminlastname" type="text" class="form-control" name="soyad" placeholder="Yönetici Soyadı">
                </div>
                <!-- email -->
                <div style="margin-bottom: 25px" class="input-group">
                    <span class="input-group-addon"><i class="glyphicon glyphicon-user"></i></span>
                    <input id="email" type="email" class="form-control" name="email" placeholder="E-mail">
                </div>
                <!-- kullaniciadi -->
                <div style="margin-bottom: 25px" class="input-group">
                    <span class="input-group-addon"><i class="glyphicon glyphicon-lock"></i></span>
                    <input id="YoneticikullaniciAdi" type="text" class="form-control" name="YoneticiKullaniciAdi" placeholder="Yönetici Kullanıcı Adı">
                </div>
                <!-- sifre -->
                <div style="margin-bottom: 25px" class="input-group">
                    <span class="input-group-addon"><i class="glyphicon glyphicon-lock"></i></span>
                    <input id="password" type="password" class="form-control" name="sifre" placeholder="Yönetici Şifre">
                </div>
                    <input type="submit" class="btn btn-success" value="Ekle">
                    
                </form>
            </div>

            <div class="panel-footer clearfix">
                <div class="pull-right">
                  

                </div>    

            </div>
        </div>
    </div>
       
    </div>

</div>
